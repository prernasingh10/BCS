<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:ma="urn:import:gwt.material.design.addins.client">
           
           
           <m:MaterialContainer ui:field="x">
    <m:MaterialRow addStyleNames="code">
            <m:MaterialTitle title="Farmer Contact Details" paddingLeft="550"  />
            <m:MaterialRow paddingLeft="500">
             <m:MaterialButton ui:field="show" text="Show details"  width="45%"/>
             </m:MaterialRow>
            <m:MaterialTextBox ui:field="name"  grid="s12" label="Name" width="40%"></m:MaterialTextBox>
            <m:MaterialTextBox  ui:field="add"  label="Address" width="40%" grid="s12"></m:MaterialTextBox>
            <m:MaterialTextBox ui:field="mob"  label="mobile no." width="40%" grid="s12"></m:MaterialTextBox>
            <m:MaterialTextBox ui:field="email"  label="Emailid"  width="40%" grid="s12"></m:MaterialTextBox>
            <m:MaterialTextBox ui:field="fid" label="Farmer Id"  width="40%" grid="s12"></m:MaterialTextBox>
            <m:MaterialTextBox ui:field="aadhar"  label="Aadhar"  width="40%" grid="s12"></m:MaterialTextBox>
                
                </m:MaterialRow>
                <m:MaterialRow>
               
                <m:MaterialColumn paddingLeft="250">
                  <m:MaterialButton ui:field="back" text="Back"></m:MaterialButton>
				</m:MaterialColumn> 
                <m:MaterialColumn paddingLeft="250">
                <m:MaterialButton ui:field="book" text="Book"></m:MaterialButton>
                </m:MaterialColumn>
               <m:MaterialColumn paddingLeft="250">
                 <m:MaterialButton ui:field="chat" text="chat"  ></m:MaterialButton>
              	</m:MaterialColumn>
              
</m:MaterialRow>
    </m:MaterialContainer>
</ui:UiBinder>