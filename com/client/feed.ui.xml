<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:ma="urn:import:gwt.material.design.addins.client"
             xmlns:incubator="urn:import:gwt.material.design.incubator.client">
            
            <m:MaterialContainer ui:field="c">
            <m:MaterialTitle paddingLeft="560" title="FEED ME!"/>
            <m:MaterialRow addStyleNames="code">
           

            <ma:masonry.MaterialMasonry backgroundColor="GREEN">
            
                    <m:MaterialCard marginBottom="0" marginTop="0" paddingLeft="480" >
                      <m:MaterialCardContent>
                        
                       
                           <m:MaterialRow paddingRight="20" paddingTop="0" >
                           <m:MaterialLink text="In love with this site? or hate it? "  iconType="FAVORITE" textColor="GREEN" />
             <m:MaterialTextBox ui:field="feed" label="feedback"  iconType="FEEDBACK" width="55%" /> 
		 
		</m:MaterialRow>
	
     <m:MaterialRow  paddingLeft="60" paddingTop="0">
      
     <m:MaterialButton  ui:field="submit" backgroundColor="GREEN"  waves="LIGHT" text="Submit" width="35%"/>
   
       </m:MaterialRow>
      
                </m:MaterialCardContent>
                    </m:MaterialCard>
               
               </ma:masonry.MaterialMasonry>
   </m:MaterialRow>
    
     </m:MaterialContainer> 
</ui:UiBinder>