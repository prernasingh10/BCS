<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:ma="urn:import:gwt.material.design.addins.client"
             xmlns:incubator="urn:import:gwt.material.design.incubator.client">
	
	
	<m:MaterialContainer ui:field="d">
	<m:MaterialRow>
	<m:MaterialRow  paddingLeft="500">
	<m:MaterialColumn>
	<m:MaterialIcon iconType="CREATE" />
	</m:MaterialColumn>
	<m:MaterialColumn>
	<m:MaterialTitle  title="Create Event"  />
	</m:MaterialColumn>
	</m:MaterialRow>
	<m:MaterialRow  paddingLeft="450">
	<m:MaterialTextBox ui:field="name" label=" Name" width="55%"/>
	 </m:MaterialRow>
	 <m:MaterialRow paddingLeft="450">
	<m:MaterialTextBox ui:field="date" placeholder="Date" width="55%" />
	</m:MaterialRow>
	<m:MaterialRow paddingLeft="450">
	<m:MaterialTextBox ui:field="loc" label="Location" width="55%"/>
	</m:MaterialRow>
	<m:MaterialRow paddingLeft="500">
	<m:MaterialColumn>
	<m:MaterialButton ui:field="submit" text="SUBMIT" />
	</m:MaterialColumn><m:MaterialColumn>
	<m:MaterialButton ui:field="back" text="Back" />
	</m:MaterialColumn>
	</m:MaterialRow>
	</m:MaterialRow>
	</m:MaterialContainer>
	</ui:UiBinder>